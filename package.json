{
  "name": "erp_blip",
  "version": "1.0.0",
  "description": "erp blip server rest api",
  "author": {
    "name": "H3llmy",
    "url": "https://github.com/h3llmy"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/h3llmy/nestjs-toko"
  },
  "license": "MIT",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "db:seed": "ts-node -r tsconfig-paths/register libs/database/src/seeder/main.ts",
    "db:seed:prod": "node dist/libs/database/src/seeder/main.js",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --detectOpenHandles",
    "test:watch": "jest --watch --detectOpenHandles",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json --detectOpenHandles",
    "test:e2e:watch": "jest --config ./test/jest-e2e.json --detectOpenHandles --watch",
    "prepare": "husky",
    "make:doc": "npx @compodoc/compodoc -p tsconfig.json -s"
  },
  "dependencies": {
    "@fastify/compress": "^7.0.3",
    "@fastify/helmet": "^11.1.1",
    "@fastify/multipart": "^8.3.0",
    "@fastify/static": "^7.0.4",
    "@nest-lab/throttler-storage-redis": "^1.0.0",
    "@nestjs/cache-manager": "^2.3.0",
    "@nestjs/common": "^10.4.7",
    "@nestjs/config": "^3.3.0",
    "@nestjs/core": "^10.4.7",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/mapped-types": "2.0.6",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-fastify": "^10.4.7",
    "@nestjs/schedule": "^4.1.1",
    "@nestjs/swagger": "^7.4.0",
    "@nestjs/terminus": "^10.2.3",
    "@nestjs/throttler": "^6.2.1",
    "@nestjs/typeorm": "^10.0.2",
    "bcryptjs": "^2.4.3",
    "cache-manager": "^5.7.6",
    "cache-manager-redis-store": "^3.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "ejs": "^3.1.10",
    "fastify-multipart": "^5.4.0",
    "glob": "^11.0.0",
    "google-auth-library": "^9.14.2",
    "ioredis": "^5.4.1",
    "nestjs-formdata-interceptor": "^1.3.1",
    "nodemailer": "^6.9.16",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.13.1",
    "qs": "^6.13.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.20"
  },
  "devDependencies": {
    "@automock/adapters.nestjs": "^2.1.0",
    "@automock/jest": "^2.1.0",
    "@nestjs/cli": "^10.4.7",
    "@nestjs/schematics": "^10.2.3",
    "@nestjs/testing": "^10.4.7",
    "@types/bcryptjs": "^2.4.6",
    "@types/ejs": "^3.1.5",
    "@types/jest": "^29.5.14",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.9.0",
    "@types/nodemailer": "^6.4.16",
    "@types/passport-jwt": "^4.0.1",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.14.0",
    "@typescript-eslint/parser": "^8.14.0",
    "eslint": "^9.14.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "husky": "^9.1.6",
    "jest": "^29.7.0",
    "prettier": "^3.3.3",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.6.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/src/",
      "<rootDir>/libs/"
    ],
    "moduleNameMapper": {
      "^@libs/common(|/.*)$": "<rootDir>/libs/common/src/$1",
      "^@libs/encryption(|/.*)$": "<rootDir>/libs/encryption/src/$1",
      "^@libs/randomize(|/.*)$": "<rootDir>/libs/randomize/src/$1",
      "^@libs/payment-gateway(|/.*)$": "<rootDir>/libs/payment-gateway/src/$1",
      "^@libs/auth-token(|/.*)$": "<rootDir>/libs/auth-token/src/$1",
      "^@libs/database(|/.*)$": "<rootDir>/libs/database/src/$1",
      "^@libs/csv(|/.*)$": "<rootDir>/libs/csv/src/$1",
      "^@domains/auth(|/.*)$": "<rootDir>/src/domains/auth/$1",
      "^@domains/permissions(|/.*)$": "<rootDir>/src/domains/permissions/$1",
      "^@domains/roles(|/.*)$": "<rootDir>/src/domains/roles/$1",
      "^@domains/users(|/.*)$": "<rootDir>/src/domains/users/$1",
      "^@libs/mail(|/.*)$": "<rootDir>/libs/mail/src/$1"
    }
  }
}