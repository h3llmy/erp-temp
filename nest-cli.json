{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": false
  },
  "projects": {
    "auth-token": {
      "type": "library",
      "root": "libs/auth-token",
      "entryFile": "index",
      "sourceRoot": "libs/auth-token/src",
      "compilerOptions": {
        "tsConfigPath": "libs/auth-token/tsconfig.lib.json"
      }
    },
    "common": {
      "type": "library",
      "root": "libs/common",
      "entryFile": "index",
      "sourceRoot": "libs/common/src",
      "compilerOptions": {
        "tsConfigPath": "libs/common/tsconfig.lib.json"
      }
    },
    "csv": {
      "type": "library",
      "root": "libs/csv",
      "entryFile": "index",
      "sourceRoot": "libs/csv/src",
      "compilerOptions": {
        "tsConfigPath": "libs/csv/tsconfig.lib.json"
      }
    },
    "database": {
      "type": "library",
      "root": "libs/database",
      "entryFile": "index",
      "sourceRoot": "libs/database/src",
      "compilerOptions": {
        "tsConfigPath": "libs/database/tsconfig.lib.json"
      }
    },
    "encryption": {
      "type": "library",
      "root": "libs/encryption",
      "entryFile": "index",
      "sourceRoot": "libs/encryption/src",
      "compilerOptions": {
        "tsConfigPath": "libs/encryption/tsconfig.lib.json"
      }
    },
    "randomize": {
      "type": "library",
      "root": "libs/randomize",
      "entryFile": "index",
      "sourceRoot": "libs/randomize/src",
      "compilerOptions": {
        "tsConfigPath": "libs/randomize/tsconfig.lib.json"
      }
    },
    "mail": {
      "type": "library",
      "root": "libs/mail",
      "entryFile": "index",
      "sourceRoot": "libs/mail/src",
      "compilerOptions": {
        "tsConfigPath": "libs/mail/tsconfig.lib.json"
      }
    }
  }
}